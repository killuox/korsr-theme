			"settings": {
				"foreground": "#bbfcee"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "keyword.control", "keyword.other.important"],
			"settings": {
				"foreground": "#aa96ff"
			}
		},
		{
			"name": "Keyword SQL",
			"scope": "keyword.other.DML",
			"settings": {
				"foreground": "#aa96ff"
			}
